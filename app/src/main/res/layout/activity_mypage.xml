        android:layout_width="453dp"
        android:layout_height="239dp"
        android:background="@drawable/mypage1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.13"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btn_version"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:background="@drawable/mypage2"
        android:fontFamily="@font/gmarketsansttfbold"
        android:gravity="left|center_vertical"
        android:paddingLeft="60dp"
        android:text="버전정보"
        app:layout_constraintEnd_toEndOf="@+id/button13"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/button13"
        app:layout_constraintTop_toBottomOf="@+id/button13" />

    <Button
        android:id="@+id/button11"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:background="@drawable/mypage2"
