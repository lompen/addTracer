        android:gravity="left|center_vertical"
        android:paddingLeft="60dp"
        android:text="내위치"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="311dp" />

    <Button
        android:id="@+id/button12"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:background="@drawable/mypage2"
        android:fontFamily="@font/gmarketsansttfbold"
        android:gravity="left|center_vertical"
        android:paddingLeft="60dp"
        android:text="내정보"
        app:layout_constraintEnd_toEndOf="@+id/button11"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/button11"
        app:layout_constraintTop_toBottomOf="@+id/button11" />

    <Button
        android:id="@+id/button13"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:background="@drawable/mypage2"
        android:fontFamily="@font/gmarketsansttfbold"
        android:gravity="left|center_vertical"
        android:paddingLeft="60dp"
        android:text="제품정보"
        app:layout_constraintEnd_toEndOf="@+id/button12"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/button12"
        app:layout_constraintTop_toBottomOf="@+id/button12" />

    <Button
        android:id="@+id/btn_inquire"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/mypage2"
        android:fontFamily="@font/gmarketsansttfbold"
        android:gravity="left|center_vertical"
        android:paddingLeft="60dp"
        android:text="문의하기"
        app:layout_constraintEnd_toEndOf="@+id/btn_version"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/btn_version"
        app:layout_constraintTop_toBottomOf="@+id/btn_version" />

    <View
        android:id="@+id/view8"
        android:layout_width="370dp"
        android:layout_height="1dp"
        android:layout_gravity="center"
